.floorWrapper .floor .titleImg {
  width: 100%;
  height: 370rpx;
  margin-top: 20rpx;
}
.floorWrapper .floor .goodsList {
  white-space: nowrap;
}
.floorWrapper .floor .goodsList .listItem {
  display: -webkit-inline-box;
  display: -webkit-inline-flex;
  display: inline-flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  width: 200rpx;
  margin: 15rpx 20rpx;
  white-space: pre-wrap;
}
.floorWrapper .floor .goodsList .listItem image {
  width: 200rpx;
  height: 200rpx;
  background-color: #eee;
}
.floorWrapper .floor .goodsList .listItem text {
  font-size: 24rpx;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  height: 64rpx;
  overflow: hidden;
}

